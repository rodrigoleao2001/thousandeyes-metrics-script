<monitor>
    <name>ThousandEyes Monitor</name>
    <type>managed</type>
    <enabled>true</enabled>
    <description>ThousandEyes Monitoring</description>
    <monitor-configuration>
    </monitor-configuration>
    <monitor-run-task>
        <execution-style>periodic</execution-style>
        <execution-frequency-in-seconds>60</execution-frequency-in-seconds>
        <execution-timeout-in-seconds>60</execution-timeout-in-seconds>
        <name>Run ThousandEyes Script</name>
        <type>executable</type>
        <task-arguments></task-arguments>
        <executable-task>
            <type>file</type>
            <file os-type="windows">run_thousandeyes_metrics.bat</file>
        </executable-task>
    </monitor-run-task>
</monitor>
